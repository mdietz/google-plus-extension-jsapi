{
  "name": "Easy G+ Circle Creator",
  "version": "0.0.1",
  "description": "Create G+ Circles from a list of profile URLS",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [ 
    "unlimitedStorage", 
    "https://plus.google.com/*"
  ],
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "img/icon32.png", 
    "default_title": "Circle Creator", 
    "default_popup": "actions/main.html"  
  }
}
